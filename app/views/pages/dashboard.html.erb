  <div class="dashboard">

    <div class="dash-items-holder">
      <div class="dashboard-items">
        <div class="dash-item">
          <div class="item-icon blue">
            <i class="fas fa-truck"></i>
          </div>
          <div class="item-info">
            <div class="item-title">
              <p> <%= Carmodel.where(classification: @commercial).count %> </p>
            </div>
            <div class="item-content">
              <p>Commercial Models</p>
            </div>
          </div>
        </div>
        <div class="dash-item">
          <div class="item-icon yellow">
            <i class="fas fa-car"></i>
          </div>
          <div class="item-info">
            <div class="item-title">
              <p> <%= Carmodel.where(classification: @retail).count %> </p>
            </div>
            <div class="item-content">
              <p>Retail Models</p>
            </div>
          </div>
        </div>
        <div class="dash-item">
          <div class="item-icon blue">
            <i class="fas fa-users"></i>
          </div>
          <div class="item-info">
            <div class="item-title">
              <p> <%= User.all.count %> </p>
            </div>
            <div class="item-content">
              <p>Staff</p>
            </div>
          </div>
        </div>

      </div>

      <div class="dashboard-items">
        <div class="dash-item1 blue">
          <div class="item-icon ">
            <i class="fas fa-laptop"></i>
          </div>
