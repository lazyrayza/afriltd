            <div class="item-number">
              <p> <%= Ahoy::Visit.all.count %> </p>
            </div>
            <div class="item-content">
              <p>Website Visits</p>
            </div>
          </div>
        </div>
        <div class="dash-item1 yellow">
          <div class="item-icon">
            <i class="fas fa-mouse"></i>
          </div>
          <div class="item-info">
            <div class="item-number">
              <p> <%= Ahoy::Event.all.count %> </p>
            </div>
            <div class="item-content">
              <p>Website Clicks</p>
            </div>
          </div>
        </div>

      </div>

    </div>

